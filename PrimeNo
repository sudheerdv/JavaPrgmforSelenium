
public class PrimeNo 
{
	public static void main(String[] args) 
	{
		int num=44;
		boolean isPrime = true;
		
		
		for(int i=2;i<num;i++)
		{
			if((num%i)==0)
			{
				isPrime=false;
				break;
			}
		}
		
		if(isPrime==true)
		{
			System.out.println(" given is a Prime No");
		}
		else
		{
			System.out.println("is not a Prime No");
		}
	} 
	
	

}
